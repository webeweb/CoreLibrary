{
    "name": "webeweb/core-library",
    "description": "Core library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "webeweb",
        "library",
        "adoria",
        "curl",
        "ftp",
        "quadratus",
        "skidata",
        "widget",
        "cipher",
        "datatabase",
        "exception",
        "logger",
        "math",
        "security",
        "sorter",
        "trait",
        "utility",
        "validator"
    ],
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "homepage": "https://github.com/webeweb/core-library",
    "support": {
        "issues": "https://github.com/webeweb/core-library/issues"
    },
    "require": {
        "php": "^7.1|^8.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-zip": "*",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "suggest": {
        "ext-ftp": "For usage of WBW\\Library\\Ftp\\Client classes"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\Adoria\\": "lib/adoria/",
            "WBW\\Library\\Curl\\": "lib/curl/",
            "WBW\\Library\\Ftp\\": "lib/ftp/",
            "WBW\\Library\\OcrLad\\": "lib/ocrlad/",
            "WBW\\Library\\Quadratus\\": "lib/quadratus/",
            "WBW\\Library\\SkiData\\": "lib/skidata/",
            "WBW\\Library\\Widget\\": "lib/widget/",
            "WBW\\Library\\Common\\": "src"
        },
        "exclude-from-classmap": [
            "/lib/adoria/Tests/",
            "/lib/curl/Tests/",
            "/lib/ftp/Tests/",
            "/lib/ocrlad/Tests/",
            "/lib/quadratus/Tests/",
            "/lib/skidata/Tests/",
            "/lib/widget/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\Adoria\\Tests\\": "lib/adoria/Tests",
            "WBW\\Library\\Curl\\Tests\\": "lib/curl/Tests",
            "WBW\\Library\\Ftp\\Tests\\": "lib/ftp/Tests",
            "WBW\\Library\\OcrLad\\Tests\\": "lib/ocrlad/Tests",
            "WBW\\Library\\Quadratus\\Tests\\": "lib/quadratus/Tests",
            "WBW\\Library\\SkiData\\Tests\\": "lib/skidata/Tests",
            "WBW\\Library\\Widget\\Tests\\": "lib/widget/Tests",
            "WBW\\Library\\Common\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.x-dev"
        }
    }
}
