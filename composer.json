{
    "name": "webeweb/core-library",
    "description": "Core library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "webeweb",
        "library",
        "accounting",
        "adoria",
        "billing",
        "core",
        "curl",
        "database",
        "ftp",
        "image",
        "logger",
        "planning",
        "provider",
        "quadratus",
        "security",
        "skidata",
        "sorter",
        "symfony",
        "system",
        "traits",
        "types",
        "validator",
        "vehicle"
    ],
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "homepage": "https://github.com/webeweb/core-library",
    "support": {
        "issues": "https://github.com/webeweb/core-library/issues"
    },
    "require": {
        "php": "^7.1|^8.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-ftp": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-zip": "*",
        "doctrine/collections": "^1.5|^2.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "symfony/yaml": "^3.0|^4.0|^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "conflict": {
        "webeweb/core-bundle": "<3.20"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\Adoria\\": "lib/adoria/",
            "WBW\\Library\\Curl\\": "lib/curl/",
            "WBW\\Library\\Ftp\\": "lib/ftp/",
            "WBW\\Library\\OcrLad\\": "lib/ocrlad/",
            "WBW\\Library\\Quadratus\\": "lib/quadratus/",
            "WBW\\Library\\SkiData\\": "lib/skidata/",
            "WBW\\Library\\Common\\": "src",
            "WBW\\Library\\Core\\": "src2/core/",
            "WBW\\Library\\Dropdown\\": "src2/dropdown/",
            "WBW\\Library\\Image\\": "src2/image/",
            "WBW\\Library\\Maths\\": "src2/maths/",
            "WBW\\Library\\Planning\\": "src2/planning/",
            "WBW\\Library\\Symfony\\": "src2/symfony/",
            "WBW\\Library\\System\\": "src2/system/"
        },
        "exclude-from-classmap": [
            "/lib/adoria/Tests/",
            "/lib/curl/Tests/",
            "/lib/ftp/Tests/",
            "/lib/ocrlad/Tests/",
            "/lib/quadratus/Tests/",
            "/lib/skidata/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\Adoria\\Tests\\": "lib/adoria/Tests",
            "WBW\\Library\\Curl\\Tests\\": "lib/curl/Tests",
            "WBW\\Library\\Ftp\\Tests\\": "lib/ftp/Tests",
            "WBW\\Library\\OcrLad\\Tests\\": "lib/ocrlad/Tests",
            "WBW\\Library\\Quadratus\\Tests\\": "lib/quadratus/Tests",
            "WBW\\Library\\SkiData\\Tests\\": "lib/skidata/Tests",
            "WBW\\Library\\Common\\Tests\\": "tests",
            "WBW\\Library\\Core\\Tests\\": "tests2/core/",
            "WBW\\Library\\Dropdown\\Tests\\": "tests2/dropdown/",
            "WBW\\Library\\Image\\Tests\\": "tests2/image/",
            "WBW\\Library\\Maths\\Tests\\": "tests2/maths/",
            "WBW\\Library\\Planning\\Tests\\": "tests2/planning/",
            "WBW\\Library\\Symfony\\Tests\\": "tests2/symfony/",
            "WBW\\Library\\System\\Tests\\": "tests2/system/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.x-dev"
        }
    }
}
