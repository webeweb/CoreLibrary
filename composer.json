{
    "name": "webeweb/core-library",
    "type": "library",
    "description": "Core library",
    "keywords": [
        "algorithm",
        "argument",
        "core",
        "curl",
        "database",
        "ftp",
        "io",
        "library",
        "network",
        "security",
        "sftp",
        "skidata",
        "sorting",
        "validation",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/core-library",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-ftp": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "psr/log": "^1.0",
        "symfony/yaml": "^2.6|^3.0|^4.0|^5.0"
    },
    "conflict": {
        "webeweb/core-bundle": "<2.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\Core\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/core-library/issues"
    }
}
